@model IEnumerable<MyStoreFront1.Models.Genres>

@{
    ViewData["Title"] = "Genres";
}
<h1>Genres</h1>

        <div class="row">
            @foreach (var genre in Model)
            {
            <div class="col-3">
                <h3>@genre.Name</h3>
                <img src="@genre.ImageUrl" class="img-fluid" />

            </div>

            }
    </div>


